{
  "name": "skinwager-backend",
  "version": "0.0.1",
  "description": "Skinwager backend.",
  "main": "index.js",
  "scripts": {
    "pm2:start": "pm2 start ecosystem.config.js",
    "start": "npm run build && node dist/server.js",
    "dev": "NODE_ENV=localhost nodemon src/server.ts",
    "dev:windows": "cross-env NODE_ENV=localhost nodemon src/server.ts",
    "tsc": "tsc",
    "build": "npm-run-all tsc copy-views",
    "test": "jest",
    "lint": "eslint src/** --ext .ts,.tsx",
    "stop-games": "DB_HOST=${DB_HOST} ts-node src/scripts/StopGames.ts",
    "copy-views": "copyfiles -V -u 2 \"./src/views/*.ejs\" \"./dist/views\"",
    "audit-games": "cross-env DRY=false DB_HOST=${DB_HOST} START_DATE=2020-03-04 END_DATE=2020-03-04 ts-node .\\src\\scripts\\AuditJackpotGames.ts",
    "audit-games:dry": "cross-env DRY=true DB_HOST=${DB_HOST} START_DATE=2020-03-03 END_DATE=2020-03-03 ts-node .\\src\\scripts\\AuditJackpotGames.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "dependencies": {
    "@types/cookie-parser": "^1.4.2",
